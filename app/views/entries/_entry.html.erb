<li>
  <p>
    <%= link_to entry do %>
      <span class="title"><%= entry.title %></span>
    <% end %>
     - Posted by <%= link_to entry.user.name %>
  </p>
  <p class="content"><%= entry.content %></p>
  <span class="timestamp">
    Posted <%= time_ago_in_words(entry.created_at) %> ago.
  </span>
</li>
